      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "express-rate-limit": "^5.1.3",
      "helmet": "^3.23.3",
      "jsonwebtoken": "^8.5.1",
      "multer": "^1.4.2",
      "node-schedule": "^1.3.2",
      "nodemailer": "^6.4.10",
      "pg": "^8.0.0",
      "pg-escape": "^0.2.0",
      "postgres": "^1.0.2",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.24",
      "yarn": "^1.22.4"
   }
